{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "desc": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "quantity": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "required": true,
      "default": "1"
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "New",
        "Added",
        "Old"
      ],
      "default": "New",
      "required": true
    },
    "has_discount": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "discount_price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "shoes_sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shoes-size.shoes-size"
    },
    "desc_short": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "currency": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::currency.currency"
    }
  }
}
